  "launch": {
    "configurations": [
      {
        "args": [],
        "askParameters": true,
        "name": "Debug current file with rdbg",
        "request": "launch",
        "script": "${file}",
        "type": "rdbg",
        "useTerminal": true
      },
      {
        "args": [],
        "askParameters": false,
        "command": "rspec",
        "name": "Debug current test with rdbg",
        "request": "launch",
        "script": "${file}",
        "type": "rdbg"
      },
      {
        "name": "Attach with rdbg",
        "request": "attach",
        "type": "rdbg"
      },
      {
        "console": "integratedTerminal",
        "justMyCode": true,
        "name": "Python: Current File",
        "program": "${file}",
        "request": "launch",
        "type": "python"
      },
      {
        "mode": "auto",
        "name": "Go: Current File",
        "program": "${fileDirname}",
        "request": "launch",
        "type": "go"
      }
    ],
    "version": "0.2.0"
  },
