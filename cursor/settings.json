  "go.testFlags": [
    "-v",
    "-count=1",
    "-timeout=120s"
  ],
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "ui.completion.usePlaceholders": false,
    "ui.semanticTokens": true,
    "ui.semanticTokenTypes": {
      // For Grammars extentions
