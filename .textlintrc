{
  "filters": {},
  "rules": {
    "preset-ja-spacing": true,
    "preset-ja-technical-writing": {
      "max-ten": {
        max: 4
      },
      "ja-no-mixed-period": false,
      "sentence-length": {
        max: 100,
        skipPatterns: [
          "/\[https?:\/\/[^\]]+\]/",
        ]
      },
    },
    "spellcheck-tech-word": true
  }
}
